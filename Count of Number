package arrayprograms;

public class ArrayPrg3 {

	public static void main(String[] args) {
//		i/p: {10,20,30,40,50,60,10,30,50,10,60}
//		O/p: 3 (count of 10)
		
		int num=10;	//Say number is 10 
		int[] a={10,20,30,40,50,60,10,30,50,10,60};
		int count=0;
		for(int i=0;i<=a.length-1;i++) {
			if(a[i]==num) {
				count++;
			}
		}
		if(count>0) {
			System.out.println(count);
		}
	}
}
